<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.shareus.web.mapper.QMemberMapper">

    <resultMap type="top.shareus.domain.entity.QMember" id="QMemberResult">
        <result property="id" column="id"/>
        <result property="qq" column="qq"/>
        <result property="nickName" column="nick_name"/>
        <result property="avatrarUrl" column="avatrar_url"/>
        <result property="specialTitle" column="special_title"/>
        <result property="remark" column="remark"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectQMemberVo">
        select id,
               qq,
               nick_name,
               avatrar_url,
               special_title,
               remark,
               del_flag,
               create_by,
               create_time,
               update_by,
               update_time
        from q_member
    </sql>

    <select id="selectQMemberList" parameterType="top.shareus.domain.entity.QMember" resultMap="QMemberResult">
        <include refid="selectQMemberVo"/>
        <where>
            <if test="qq != null ">
                and qq = #{qq}
            </if>
            <if test="nickName != null  and nickName != ''">
                and nick_name like concat('%', #{nickName}, '%')
            </if>
            <if test="avatrarUrl != null  and avatrarUrl != ''">
                and avatrar_url = #{avatrarUrl}
            </if>
            <if test="specialTitle != null  and specialTitle != ''">
                and special_title = #{specialTitle}
            </if>
        </where>
    </select>

    <select id="selectQMemberById" parameterType="Long"
            resultMap="QMemberResult">
        <include refid="selectQMemberVo"/>
        where id = #{id}
    </select>

    <insert id="insertQMember" parameterType="top.shareus.domain.entity.QMember" useGeneratedKeys="true"
            keyProperty="id">
        insert into q_member
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="qq != null">qq,
            </if>
            <if test="nickName != null and nickName != ''">nick_name,
            </if>
            <if test="avatrarUrl != null">avatrar_url,
            </if>
            <if test="specialTitle != null">special_title,
            </if>
            <if test="remark != null">remark,
            </if>
            <if test="delFlag != null">del_flag,
            </if>
            <if test="createBy != null">create_by,
            </if>
            <if test="createTime != null">create_time,
            </if>
            <if test="updateBy != null">update_by,
            </if>
            <if test="updateTime != null">update_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="qq != null">#{qq},
            </if>
            <if test="nickName != null and nickName != ''">#{nickName},
            </if>
            <if test="avatrarUrl != null">#{avatrarUrl},
            </if>
            <if test="specialTitle != null">#{specialTitle},
            </if>
            <if test="remark != null">#{remark},
            </if>
            <if test="delFlag != null">#{delFlag},
            </if>
            <if test="createBy != null">#{createBy},
            </if>
            <if test="createTime != null">#{createTime},
            </if>
            <if test="updateBy != null">#{updateBy},
            </if>
            <if test="updateTime != null">#{updateTime},
            </if>
        </trim>
    </insert>

    <update id="updateQMember" parameterType="top.shareus.domain.entity.QMember">
        update q_member
        <trim prefix="SET" suffixOverrides=",">
            <if test="qq != null">qq =
                #{qq},
            </if>
            <if test="nickName != null and nickName != ''">nick_name =
                #{nickName},
            </if>
            <if test="avatrarUrl != null">avatrar_url =
                #{avatrarUrl},
            </if>
            <if test="specialTitle != null">special_title =
                #{specialTitle},
            </if>
            <if test="remark != null">remark =
                #{remark},
            </if>
            <if test="delFlag != null">del_flag =
                #{delFlag},
            </if>
            <if test="createBy != null">create_by =
                #{createBy},
            </if>
            <if test="createTime != null">create_time =
                #{createTime},
            </if>
            <if test="updateBy != null">update_by =
                #{updateBy},
            </if>
            <if test="updateTime != null">update_time =
                #{updateTime},
            </if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteQMemberById" parameterType="Long">
        delete
        from q_member
        where id = #{id}
    </delete>

    <delete id="deleteQMemberByIds" parameterType="String">
        delete from q_member where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>